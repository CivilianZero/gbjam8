--mechanics
function gettarget(e)
	local target,bdst,tdst=nil,99,99
 calcdist(e.x,e.y)
	for s in all(slimes) do
		if s.ally then
		tdst=999
	  for r in all(e.range) do
	   local dx,dy=s.x-r[1],s.y+r[2]
	   if iswalkable(dx,dy,"checkmobs") then
	   	tdst=distmap[dx][dy]
    end
			 if tdst<bdst then
			  if los(dx,dy,s.x,s.y) then
				  bdst=tdst
				  target=s
				 end
			 end
	  end
		end
	end
	return target
end

--‚òÖ
function subrange(range)
 local maxrange=sqrt(range*range)
 
	for x=0,15 do
		for y=0,15 do
			if distmap[x][y]>-1 then
				distmap[x][y]-=maxrange
			end
		end
	end
end

function findpath()
 local bx,by,path=0,0,{}
	for i=1,4 do
	 local dx,dy=dirx[i],diry[i]
	 local tx,ty=e.x+dx,e.y+dy
	 bx,by=tx,ty
	 if iswalkable(tx,ty,"checkmobs") then
		 local dst=distmap[tx][ty]
	  if dst<bdst 
	  and tx then
		  bdst=dst
	  end
	 end
 end
end
